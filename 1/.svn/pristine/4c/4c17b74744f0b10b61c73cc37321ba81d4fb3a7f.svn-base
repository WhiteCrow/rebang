	width:80px;
	margin:0px;
	font-family:helvetica;
	font-size:13px;
	border:1px solid transparent;
	padding:5px;
	background:transparent;
	color:#000;
	outline:0px;
	margin-right:5px;
	margin-bottom:5px
}
div.tagsinput div {
	display:block
}
