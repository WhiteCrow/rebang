.shadow-box {
	border:1px solid #ddd;
	border-radius:2px 2px 2px 2px;
	box-shadow:0 4px 3px -3px rgba(0,0,0,0.1);
	color:#778087;
	margin-bottom:5px;
	padding:7px 10px;
	text-shadow:0 1px #fff;
	margin-right:10px;
	background:#fff
}
.shadow-box.yellow {
	background:#FFFFCC
}
.inset-margin {
	background:none repeat scroll 0 0 #eee;
	border-radius:4px 4px 4px 4px;
	box-shadow:0 0 1px #aaa inset,0 1px 0 #fff;
	padding:7px;
	position:relative
}
