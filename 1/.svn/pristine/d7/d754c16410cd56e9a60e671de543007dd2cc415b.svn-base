header {
	display:block;
	background-color:#f7f7f7;
	border-top:1px solid #286ba7;
	padding:0px 20px 20px 20px !important;
	position:absolute;
	top:0;
	left:0;
	right:0;
	z-index:401
}
header i {
	margin-top:2px !important
}
.header-inner {
	display:block;
	clear:both;
	overflow:auto
}
.logo {
	display:block;
	float:left;
	margin:0
}
.mainNav {
	display:block;
	float:left;
	width:auto
}
.mainNav li {
	display:block;
	float:left;
	width:auto;
	margin-right:10px
}
.loginBox {
	width:200px;
	height:auto;
	float:right;
	overflow:auto
}
.loginBox>button {
	display:block;
	width:auto;
	float:right
}
.subHead {
	height:29px;
	padding:10px 20px 10px 30px;
	position:absolute;
	top:0px;
	left:0;
	right:0;
	z-index:400;
	background-color:#fbfbfb;
	position:absolute
}
.subHeadInner h3 {
	font-size:24px;
	color:#00afdb;
	float:left;
	font-weight:lighter
}
.mod_footer {
	background:#eee
}
.single_mod {
	background:white;
	margin-top:-10px;
	margin-left:-10px !important;
	padding:15px;
	border-radius:5px 0 0;
	border-right:1px solid #ddd;
	margin-bottom:-10px
}
.subHead .menuButton {
	margin:0;
	float:left
}
.subHead .menuButton i {
	margin-top:1px
}
.line {
	background:url("/static/images/36kr/line.png") no-repeat scroll center bottom transparent;
	padding-top:5px;
	padding-bottom:5px
}
.line_hr {
	border-bottom:1px solid #eee
}
.line-short {
	background:url("/static/images/36kr/line-short.png") no-repeat scroll right bottom transparent;
	padding-top:5px;
	padding-bottom:5px
}
nav {
	text-align:right
}
nav a {
	color:#444444
}
#mainContainer {
	background:url("/static/images/36kr/nav_bg.png") repeat scroll 0 0 transparent
}
#mainContainer.container-fluid {
	-moz-box-sizing:border-box;
	left:0;
	padding:0 20px 20px;
	right:0;
	width:100%;
	-webkit-box-sizing:border-box;
	-moz-box-sizing:border-box;
	-ms-box-sizing:border-box;
	-o-box-sizing:border-box;
	box-sizing:border-box
}
.bcrumbs {
	list-style:none
}
.bcrumbs li {
	display:block;
	float:left;
	font-size:22px;
	margin-left:8px;
	color:rgba(0,0,0,0.3);
	margin-right:8px;
	cursor:pointer;
	-webkit-transition:color 0.1s ease-in-out;
	-moz-transition:color 0.1s ease-in-out;
	-o-transition:color 0.1s ease-in-out;
	-ms-transition:color 0.1s ease-in-out;
	transition:color 0.1s ease-in-out
}
.bcrumbs li:hover:not(.divdot):not(last-child) {
	color:rgba(0,0,0,0.4)
}
.bcrumbs li:last-child {
	color:#00afdb;
	margin-right:0px
}
.sideBar {
	overflow-x:hidden;
	overflow-y:hidden;
	padding-top:20px
}
.sideBar a {
	color:#141F40
}
.sideBar ul {
	list-style:none;
	margin:0
}
.sideBar ul>li {
	color:#141F40;
	cursor:pointer;
	font-size:16px;
	margin-bottom:2px
}
.sideBar ul>li figure {
	padding:6px 14px 6px 14px;
	margin:0
}
.sideBar ul>li.active figure:first-child {
	color:#00afdb
}
.sideBar ul>li i {
	display:inline-block
}
.sideBar ul>li .menuText {
	display:inline-block
}
