.gallery .tile {
	padding:8px;
	border:1px #e6e6e6 solid;
	display:inline-block;
	margin:0px 10px 10px 0px;
	opacity:0.7;
	position:relative;
	-webkit-transition:opacity 0.2s ease;
	-moz-transition:opacity 0.2s ease;
	-ms-transition:opacity 0.2s ease;
	-o-transition:opacity 0.2s ease;
	transition:opacity 0.2s ease;
	overflow:hidden
}
.gallery .tile img {
	position:absolute;
	top:8px;
	left:8px;
	z-index:31
}
.gallery .tile:hover {
	box-shadow:0px 0px 10px 0px rgba(0,0,0,0.1);
	opacity:1
}
.gallery .tile:hover .hoverImg {
	opacity:1
}
.gallery .tile .hoverImg {
	position:absolute;
	top:40px;
	left:0;
	right:0;
	height:30px;
	z-index:32;
	opacity:0;
	background-color:rgba(0,0,0,0.1);
	text-align:center;
	padding-top:6px;
	color:#FFF;
	-webkit-transition:opacity 0.2s ease;
	-moz-transition:opacity 0.2s ease;
	-ms-transition:opacity 0.2s ease;
	-o-transition:opacity 0.2s ease;
	transition:opacity 0.2s ease
}
